// https://www.lanqiao.cn/problems/5118/learning/?page=1&first_category_id=1&name=%E5%B0%8F%E5%85%B0%E7%9A%84%E7%A5%9E%E7%A7%98%E7%A4%BC%E7%89%A9

#include <iostream>
#include <vector>
#include <queue>
using namespace std;

#define maxn 1001
#define maxvn 1001

int volume[maxn];
int value[maxn];
// 注意只能从1开始
int dp[maxn][maxvn];

void napsack01(int n, int cap) {
    for (int i = 1; i <= n; ++i) {
        dp[0][i] = 0;
    }
    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= cap; ++j) {
            if (volume[i] <= j) {
                // 为了匹配题目，需修改这里
                dp[i][j] = max(dp[i - 1][j], dp[i - 1][j - volume[i]] + volume[i]);
            } else {
				        dp[i][j] = dp[i - 1][j];
            }
        }
    }
}

int main()
{
    int n, cap;
    cin >> cap;
    cin >> n;
    for (int i = 1;i <= n; i++) {
		cin >> volume[i];  
		value[i] = 1;
    }
    napsack01(n, cap);
    int ans = 0;
    for (int i = 1; i <= cap;++i) {
        ans = max(ans, dp[n][i]);
    }
    cout << cap - ans << endl;
    return 0;
}
